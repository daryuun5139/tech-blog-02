{
  "name": "my-tech-blog",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "debug": "NODE_OPTIONS='--inspect' next dev",
    "lint": "npx run lint:prettier && npx run lint:es && npx run lint:style ",
    "lint:prettier": "prettier --check --loglevel=warn '**/*.{js,ts,jsx,tsx,mdx,scss}'",
    "lint:es": "eslint '**/*.{js,ts,jsx,tsx,mdx}'",
    "lint:style": "stylelint '**/*.{css,scss}'",
    "format": "npx run format:prettier && npx run format:es && npx run format:style",
    "format:prettier": "prettier --check --write --loglevel=warn '**/*.{js,ts,jsx,tsx,mdx,scss}'",
    "format:es": "eslint --fix '**/*.{js,ts,jsx,tsx,mdx}'",
    "format:style": "stylelint --fix '**/*.{css,scss}'"
  },
  "dependencies": {
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@next/bundle-analyzer": "^13.5.3",
    "@types/node": "20.4.5",
    "@types/react": "18.2.18",
    "@types/react-dom": "18.2.7",
    "accept-language": "^3.0.18",
    "autoprefixer": "10.4.14",
    "dayjs": "^1.11.9",
    "eslint": "8.46.0",
    "eslint-config-next": "13.4.12",
    "highlight.js": "^11.8.0",
    "html-react-parser": "^4.2.0",
    "i18next": "^23.4.6",
    "i18next-resources-to-backend": "^1.1.4",
    "microcms-js-sdk": "^2.5.0",
    "next": "13.4.12",
    "next-themes": "^0.2.1",
    "postcss": "8.4.27",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-i18next": "^13.2.1",
    "tailwindcss": "3.3.3",
    "typescript": "5.1.6"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^6.2.1",
    "@typescript-eslint/parser": "^6.2.1",
    "eslint-config-prettier": "^8.10.0",
    "prettier": "^3.0.1",
    "prettier-plugin-tailwindcss": "^0.4.1",
    "stylelint": "^15.10.2",
    "stylelint-config-prettier-scss": "^1.0.0",
    "stylelint-config-recess-order": "^4.3.0",
    "stylelint-config-standard-scss": "^10.0.0"
  }
}
